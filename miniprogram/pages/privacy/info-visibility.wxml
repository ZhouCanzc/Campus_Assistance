<!--pages/privacy/info-visibility.wxml-->
<view class="setting-container">
  <view class="privacy-list">
    <view class="setting-item">

      <text class="setting-lable">联系方式可见性</text>

      <switch class="item-right" checked="{{item.checked}}" bindchange="handleAuthChange" data-type="{{item.type}}"
        disabled="{{item.disabled}}" color="#07c160" />
    </view>

    <view class="setting-item">
      <view class="setting-desc">开启后，仅接单者可查看您的联系方式</view>
    </view>

  </view>
</view>