/* pages/privacy/info-visibility.wxss */
.setting-container {
  background-color: #f5f5f5;
  min-height: 100vh;
}

.setting-list {
  background-color: #fff;
  margin-top: 8px;
}

.setting-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 8px;
  background-color: #fff;
  border-bottom: 1px solid #eee;
}

.setting-label {
  font-size: 16px;
  color: #0f0f0f;
}

.item-right{
  transform: scale(0.9);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 8px;
  border-bottom: 1px solid #eee;
  text-align: left;
}

.setting-desc{
  color: #999;
  font-size: 14px;
  margin-right: 10px;
  flex: 1;
  text-align: left;
}